#include<bits/stdc++.h>

bool isValidParenthesis(string s)
{
    // Write your code here.
    stack<char> st;
    map<char,int> b = {
        {'{',1},{'}',-1},
        {'(',2},{')',-2},
        {'[',3},{']',-3}
    };
    for(int i = 0; i < s.size(); i++){
        if(b[s[i]] > 0){
            st.push(b[s[i]]);
        }else{
            if(st.empty()){
                return false;
            }
            else if(b[s[i]] + st.top() == 0){
                st.pop();
            }else{
                return false;
            }
        }
    }
    return st.empty();
}