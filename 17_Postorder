void postorder(TreeNode *node, vector<int>& ans){
    if(node == NULL){
        return;
    }

    postorder(node->left, ans);
    postorder(node->right, ans);
    ans.push_back(node->data);
}

vector<int> getPostOrderTraversal(TreeNode *root)
{
    // Write your code here.
    vector<int> ans;
    postorder(root, ans);
    return ans;
}